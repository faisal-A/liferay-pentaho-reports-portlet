<?xml version="1.0"?>

<faces-config
    version="2.0"
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
>
	<application>
		<resource-handler>com.mycompany.reports.resource.ReportHTMLResourceHandler</resource-handler>
		<resource-handler>com.mycompany.reports.resource.ReportPDFResourceHandler</resource-handler>
		<resource-handler>com.mycompany.reports.resource.ReportExcelResourceHandler</resource-handler>
	</application>
	<lifecycle>
		<phase-listener>com.liferay.faces.util.lifecycle.DebugPhaseListener</phase-listener>
	</lifecycle>
</faces-config>